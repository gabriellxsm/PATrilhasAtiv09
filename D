using System;

class Program
{
    static void Main()
    {
        int[] vetor = { 3, 5, 8, 2, 6 };
        int maiorValor = vetor[0];

        foreach (int valor in vetor)
        {
            if (valor > maiorValor)
            {
                maiorValor = valor;
            }
        }

        Console.WriteLine("O maior valor no vetor Ã©: " + maiorValor);
    }
}
