using System;

class Program
{
    static void Main()
    {
        int[] vetor = { 3, 5, 8, 2, 6 };
        int menorValor = vetor[0];

        foreach (int valor in vetor)
        {
            if (valor < menorValor)
            {
                menorValor = valor;
            }
        }

        Console.WriteLine("O menor valor no vetor Ã©: " + menorValor);
    }
}
